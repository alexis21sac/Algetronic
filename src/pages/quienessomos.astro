    @keyframes blur{
        to{
            color:rgb(231, 235, 239);
            box-shadow: 
                0px 5px 50px -5px hsla(240, 9%, 98%, 0.465),
                0px 0px 0px 1px hsla(203, 71%, 73%, 0.317);
